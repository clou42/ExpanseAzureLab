  # Keep in mind: Not all regions offer all machine types. Not all regions are availiable from everythere in the world
  region = "westeurope"
  # Public IP for firewall whitelisting on DBs and VMs. 
  client_ip = "198.51.100.1"
  # Subscription ID to deploy in 
  subscription_id = "12345678-1234-1234-1234-123456781234"
  # Resource group name to create for the Lab
  resource_grp_name = "ExpanseAzureSecLab"
  # SSH Key to access the Rocinante VM
  rocinante_ssh_key = "ssh-rsa "
  # SSH Key to access the Scopuli VM
  scopuli_ssh_key = "ssh-rsa "
  # Username to access the Scopuli VM via SSH
  scopuli_ssh_user = "darren"
  # DB SA on Tycho MSSQL Server
  tycho_sa_username = "fred"
  # DB SA on Tycho MSSQL Server password
  tycho_sa_password = "[tycho_sa_password]"
